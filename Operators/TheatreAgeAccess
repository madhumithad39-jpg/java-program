import java.util.Scanner;

public class TheatreAgeAccess {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int age =s.nextInt();
        int idproof = s.nextInt();
        if(age>=18&&idproof==1){
            System.out.println("Allowed");
        }
        else{
            System.out.println("Denied");
        }
    }
}
